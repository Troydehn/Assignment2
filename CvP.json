{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "A bar chart showing the US population distribution of age groups and gender in 2000.",
  "data": {"url": "https://raw.githubusercontent.com/Troydehn/Assignment2/master/Processing/crime_vs_population.csv"},
  "width": 400,
  "height": 125,
  "mark": "bar",
  "encoding": {
    "x": {"field": "jurisdiction", "type": "nominal"},
    "y": {
      "aggregate": "sum",
      "field": "Value",
      "title": "Value",
      "stack": true
    },
    "color": {"field": "Value Type", "scale": {"range": ["#675193", "#ca8861"]}},
    "opacity": {"value": 0.7}
  }
}