{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/Troydehn/Assignment2/master/Processing/division_of_crimes.csv"},
    "mark": "bar",
    "width": 400,
    "height": 125,
    "encoding": {
      "x": {
        "field": "jurisdiction",
        "title": "State",
        "type": "nominal"
      },
      "y": {
        "field": "Value",
        "type": "quantitative"
      },
      "fillOpacity": {
        "value": 1
      },
      "color": {
        "field": "Crime Type",
        "type": "nominal",
        "scale": {
          "domain": ["agg_assault", "robbery", "rape_revised",  "murder_manslaughter"],
          "range": ["#e7ba52", "#c7c7c7", "#aec7e8"]
        }
      }

      
    }
  }